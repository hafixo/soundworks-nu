<?xml version='1.0' encoding='utf-8'?>
<widget id="io.cordova.woodlands" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Woodlands</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="davipoir@ircam.fr" href="http://cosima.ircam.fr/">
        IRCAM - COSIMA project
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
        <icon src="assets/Icon-60@2x.png" width="120" height="120" />
    </platform>
</widget>
